local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
  local Window = Library.CreateLib("SpaceGG | MeepCity", "Ocean")
  local Tab = Window:NewTab("Main Stuff")
  local Section = Tab:NewSection("Main Stuff")
  Section:NewButton("Plus", "Gives You Plus", function()
    game.ReplicatedStorage.PlayerData[game.Players.LocalPlayer.UserId].PLUS.Value = true
end)


local Tab = Window:NewTab("Inf Money")
  local Section = Tab:NewSection("Inf Money")
Section:NewButton("Money Hack", "U need to join starball obby and execute this!", function()
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v:IsA("Part") then
    local number_1 = 8;
    local userdata_1 = v;
    local number_2 = 5000;
    local number_3 = math.huge;
    local number_4 = 5000;
    local Target = game:GetService("ReplicatedStorage").Connection;
    Target:InvokeServer(number_1, userdata_1, number_2, number_3, number_4);
    end
    end
    wait(5)
    game.Players.LocalPlayer:Kick("Rejoin the MeepCity")
end)

local Tab = Window:NewTab("Local Player")
local Section = Tab:NewSection("Local Player")

Section:NewSlider("WalkSpeed", "Sets Ur WalkSpeed", 250, 16, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("JumpPower", "Sets Ur JumpPower", 350, 50, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
